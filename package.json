{
  "name": "megalo-frame",
  "version": "0.0.0",
  "description": "a megalo project",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev:alipay": "webpack --config ./build/webpack.config.alipay.js -w",
    "dev:swan": "webpack --config ./build/webpack.config.swan.js -w",
    "dev:wechat": "webpack --config ./build/webpack.config.wechat.js -w",
    "dev:web": "webpack-dev-server --config ./build/webpack.config.web.dev.js",
    "prod:web": "webpack --config ./build/webpack.config.web.prod.js"
  },
  "keywords": [],
  "author": "陈诗文 <mosquito1403@163.com>",
  "license": "ISC",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@megalo/entry": "^0.1.1",
    "@megalo/px2rpx-loader": "^0.1.1",
    "@megalo/template-compiler": "^0.7.3",
    "@types/jest": "^24.0.11",
    "@types/webpack-env": "^1.13.9",
    "autoprefixer": "^9.4.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "copy-webpack-plugin": "^4.5.2",
    "css": "^2.2.4",
    "css-loader": "^1.0.0",
    "cypress": "^3.2.0",
    "directory-named-webpack-plugin": "4.0.0",
    "eslint": "^5.14.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "lerna": "^3.13.1",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.10.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4000",
    "url-loader": "^1.1.2",
    "vue-loader": "15.4.0",
    "vue-style-loader": "^4.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.14",
    "@olympus-test/main": "latest"
  },
  "dependencies": {
    "@megalo/api": "^0.2.3",
    "@megalo/target": "^0.7.0",
    "@megalo/ui": "^1.0.0",
    "@megalo/vhtml-plugin": "^0.1.2",
    "megalo": "^0.9.1-0",
    "octoparse": "^0.3.2",
    "vue": "2.5.17",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "2.5.17",
    "vuex": "^3.1.0",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0"
  }
}
